<filter name="insert" module="wiki" act="procWikiInsertDocument" confirm_msg_code="confirm_submit">
  <form>
    <node target="title" required="true" minlength="1" maxlength="250" />
    <node target="content" required="true" />
  </form>
  <response>
    <tag name="error" />
    <tag name="message" />
  </response>
</filter>
